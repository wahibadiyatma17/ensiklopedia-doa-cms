{
  "kind": "collectionType",
  "collectionName": "doas",
  "info": {
    "singularName": "doa",
    "pluralName": "doas",
    "displayName": "Doa",
    "description": "Muslim prayers and supplications"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rank": {
      "type": "integer",
      "default": 0,
      "configurable": false,
      "writable": true,
      "visible": true,
      "private": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "doa"
    },
    "doa": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}

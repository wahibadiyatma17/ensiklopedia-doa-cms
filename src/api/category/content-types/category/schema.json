{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "settings": {
    "defaultSortBy": "rank",
    "defaultSortOrder": "asc"
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "subcategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "categories"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "subcategories"
    },
    "doa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doa.doa",
      "mappedBy": "category"
    },
    "rank": {
      "type": "integer",
      "default": 0,
      "configurable": false,
      "writable": true,
      "visible": true,
      "private": false
    }
  }
}
